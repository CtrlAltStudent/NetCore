{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/tests/NetCore.FinancialEngine.Tests/NetCore.FinancialEngine.Tests.csproj", "/property:GenerateFullPaths=true", "/consoleloggerparameters:NoSummary;ForceNoAlign"],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-maui",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/src/NetCore.Maui/NetCore.Maui.csproj", "-f", "net10.0-windows10.0.19041.0", "/property:GenerateFullPaths=true", "/consoleloggerparameters:NoSummary;ForceNoAlign"],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish",
      "command": "dotnet",
      "type": "process",
      "args": ["publish", "${workspaceFolder}/tests/NetCore.FinancialEngine.Tests/NetCore.FinancialEngine.Tests.csproj", "/property:GenerateFullPaths=true", "/consoleloggerparameters:NoSummary;ForceNoAlign"],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "args": ["watch", "run", "--project", "${workspaceFolder}/tests/NetCore.FinancialEngine.Tests/NetCore.FinancialEngine.Tests.csproj"],
      "problemMatcher": "$msCompile"
    }
  ]
}
